<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 设置为IUserDao接口方法提供sql语句配置 -->
<mapper namespace="com.zzt.dao.IApplyOrderDao">
    <insert id="addApplyOrder"   parameterType="ApplyOrder">
        insert into applyorder (createdate,type,approverid,applicantid,operation,sid,comm)
        values (#{createdate},#{type},#{approverid},#{applicantid},#{operation},#{sid},#{comm})
    </insert>

    <delete id="deleteApplyOrder"  parameterType="integer">
        delete from applyorder where num= #{num}
    </delete>

</mapper>