<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 设置为IUserDao接口方法提供sql语句配置 -->
<mapper namespace="com.zzt.dao.IOrderDao">

    <select id="addOrder"  resultType="integer" parameterType="ApplyOrder">
        insert into applyorder (createdate,type,comm,approverid,applicantid)
        values (#{date},#{type},#{comm},#{approverid},#{applicantid})
    </select>

</mapper>